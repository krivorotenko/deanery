<?xml version="1.0" encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name = "deanery.Student" table = "student">
     <id column="id_student" name="id" type="java.lang.Integer">
	    <generator class="native"/>
	 </id>	 
	 <property column="name" name="name" type="java.lang.String"/>
	 <property column="surname" name="surname" type="java.lang.String"/>
	  <property column="GPA" name="GPA" type="java.lang.Double"/>
	  <property column="id_group" name="group" type="java.lang.Integer"/>
     </class>
</hibernate-mapping>